{
    "compilerOptions": {
        "outDir": "./dist/",
        "module": "commonjs",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "moduleResolution": "node",
        "allowJs": true,
        "target": "es5",
        "lib": ["es5","es6","dom"],
        "jsx": "react",
    },
    "include": [
        "./src/**/*"
    ]
}